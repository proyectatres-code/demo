{
  "headers": [
    {
      "source": "/",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, s-maxage=300" },
        { "key": "Accept-Ranges", "value": "none" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, s-maxage=300" },
        { "key": "Accept-Ranges", "value": "none" }
      ]
    },
    {
      "source": "/imagenes/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Accept-Ranges", "value": "none" }
      ]
    }
  ]
}
